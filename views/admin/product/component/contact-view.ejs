<% 
  var cname = ""
  var cemail = ""
  var cphone = ""
  var ccountry = ""
  if (contact != undefined && contact != null) {
    cname = contact.name 
    cemail = contact.email
    cphone = contact.phone
    ccountry = contact.country
  } %>

<div class="card accordion-item mb-4">
  <h2 class="accordion-header" id="contactTwo">
    <button type="button" class="accordion-button" data-bs-toggle="collapse" data-bs-target="#contact-view"
      aria-expanded="false" aria-controls="contact-view">
      Liên Hệ
    </button>
  </h2>
  <div id="contact-view" class="accordion-collapse collapse show" aria-labelledby="contactTwo" data-bs-parent="#contact-view"
    style="">
    <div class="accordion-body">
      <div class="mb-3">
        <label class="form-label" for="contact-name">Tên</label>
        <input type="text" class="form-control" id="contact-name" name="cname" value="<%= cname %>" placeholder="Title">
      </div>
      <div class="mb-3">
        <label class="form-label" for="contact-email">Email</label>
        <input type="text" class="form-control" id="contact-email" name="cemail" value="<%= cemail %>" placeholder="Name">
      </div>
      <div class="mb-3">
        <label class="form-label" for="contact-phone">Phone</label>
        <input type="text" class="form-control" id="contact-phone" name="cphone" value="<%= cphone %>" placeholder="Keys">
      </div>
      <div class="mb-3">
        <label class="form-label" for="contact-country">Địa chỉ</label>
        <input type="text" class="form-control" id="contact-country" name="ccountry" value="<%= ccountry %>" placeholder="Country">
      </div>
    </div>
  </div>
</div>