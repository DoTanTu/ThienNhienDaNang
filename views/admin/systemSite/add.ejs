<!DOCTYPE html>
<html lang="en" class="light-style layout-menu-fixed" dir="ltr" data-theme="theme-default"
  data-assets-path="../public/admin/assets/" data-template="vertical-menu-template-free">

<head>
  <%- include('../partials/head') -%>
  <%- include('../partials/common-crud-css') -%>
</head>

<body>
  <!-- Layout wrapper -->
  <div class="layout-wrapper layout-content-navbar">
    <div class="layout-container">
      <%- include('../partials/navbar-left') -%>
        <!-- Layout container -->
        <div class="layout-page">
          <%- include('../partials/navbar-top') -%>
            <!-- Content wrapper -->
            <div class="content-wrapper">
              <div class="container-xxl flex-grow-1 container-p-y ">
                <!-- ============================= -->
                <!-- Content Site -->
                <!-- ============================= -->
                

                <form class="browser-default-validation j-pro" id="formSiteData" action="/admin.addSite" method="POST">
                  <div class="row">
                    <div class="col-lg-8 col-12 mb-lg-0 mb-4">
                      <div class="card">

                        <h5 class="card-header">Site Data</h5>
                        <div class="card-body">
                          <div class="mb-3">
                            <label class="form-label" for="site-name">Site Name</label>
                            <input type="text" class="form-control" hidden name="appId" value="<%= appId%>">
                            <input type="text" class="form-control" id="site-name" name="name" oninput="onChangeSiteName(this)"
                              placeholder="Site name" required="">
                          
                          </div>
                          <div class="mb-3">
                            <label class="form-label" for="site-url">Site Url</label>
                            <input type="text" class="form-control" id="site-url" name="url"
                              placeholder="Site url" required="">
                          </div>

                          <%- include('./component/add-image') -%>
                          <%- include('./component/add-css') -%>
                          <%- include('./component/add-js') -%>
                          <%- include('./component/add-html', {pathHtml : ""}) -%>
                         

                          <div class="row">
                            <div class="col-12">
                              <button type="submit" id="submit" class="btn btn-primary">Submit</button>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                    <!-- /Invoice Add-->

                    <div class="col-lg-4 col-12">
                      <%- include('./component/setting-view', {settingData : null, subMenuCustomData : null}) -%>
                      <%- include('./component/page-data-view', {pageData : null}) -%>
                      <%- include('./component/page-data-other1-view', {pageOther1 : null}) -%>
                      <%- include('./component/page-data-other2-view', {pageOther2 : null}) -%>
                      <%- include('./component/page-data-other3-view', {pageOther3 : null}) -%>
                      <%- include('./component/page-data-other4-view', {pageOther4 : null}) -%>
                      <%- include('./component/page-data-other5-view', {pageOther5 : null}) -%>
                      <%- include('./component/page-data-other6-view', {pageOther6 : null}) -%>
                      <%- include('./component/page-data-other7-view', {pageOther7 : null}) -%>
                      <%- include('./component/page-data-other8-view', {pageOther8 : null}) -%>
                      <%- include('./component/page-data-other9-view', {pageOther9 : null}) -%>
                      <%- include('./component/page-data-other10-view', {pageOther10 : null}) -%>
                     
                      <%- include('../component/seoView',  {seoData : null}) -%>
                    </div>
                  </div>
                </form>
                <!-- ============================= -->
                <!-- Content Site -->
                <!-- ============================= -->
              </div>
              <!-- Footer -->
              <%- include('../partials/footer') -%>
                <!-- / Footer -->
                <div class="content-backdrop fade"></div>
            </div>
            <!-- Content wrapper -->
        </div>
        <!-- / Layout Site -->
    </div>
    <!-- Overlay -->
    <div class="layout-overlay layout-menu-toggle"></div>
  </div>
  <!-- / Layout wrapper -->
    <%- include('../partials/common-js') -%>
    <script type="text/javascript" src="../public/admin/assets/libs/j-pro/js/jquery.ui.min.js"></script>
    <script type="text/javascript" src="../public/admin/assets/libs/j-pro/js/jquery.maskedinput.min.js"></script>
    <script type="text/javascript" src="../public/admin/assets/libs/j-pro/js/jquery.j-pro.js"></script>
    <script type="text/javascript" src="../public/admin/assets/js/jquery.form.min.js"></script>

    <script src="../public/admin/assets/libs/dropzone/dropzone.min.js"></script>
    <script src="../public/admin/assets/js/pages/files/uploadSite.js"></script>
    <script src="../public/admin/assets/js/pages/files/uploadHtml.js"></script>

    <script src="../public/admin/assets/js/pages/systemSite/add.js"></script>
    <script src="../public/admin/assets/js/pages/systemSite/setting-view.js"></script>
   
</body>

</html>