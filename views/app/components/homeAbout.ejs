<div class="home-about py-90 bg-grey" id="about-us">
    <div class="container">
        <% if (dataCategories) {
            const category = dataCategories.find(category => category._id === 'WSIqFEUod');
            if (category) {
                    name = category.name,
                    desc = category.desc
             %>
        <h3 class="h3-title mb-3 text-center"><%= name.toUpperCase()%></h3>
        <p class="p-title mb-5"><%= desc%></p>
            <%}
        }%>
        <div class="row-change">
            <%if(dataProducts ){
                dataProducts.forEach((prod) => {
                    if(prod.categoryIds[0] === 'WSIqFEUod'){
                        nameProd = prod.name;
                        let alt = '';
                        let pathImage = '';
                        let imageResize = '';
                        const obImage = prod.images[0];
                        if(obImage){
                            pathImage = obImage.image;
                            alt = obImage.alt;
                        }
                        imageResize = pathImage.replace('images/', 'thumbnail/480/').replace(/\.(png|jpg|jpeg)$/i, '.webp');
              %>
            <div class="cot-12 cot-4-md px-30 py-30 home-about-box">
                <img class="mb-3" src="<%= imageResize%>" onerror="javascript:this.src='<%= pathImage%>'" alt="<%= alt%>">
                <h4 class="text-center"><%- nameProd%></h4>
            </div>
             <%  }
              });
            }%>
        </div>
    </div>
</div>