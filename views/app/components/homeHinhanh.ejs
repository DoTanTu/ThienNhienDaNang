<div class="home-hinhanh py-90">
    <div class="container">
        <h3 class="h3-title mb-3 text-center">Hình ảnh</h3>
        <p class="p-title mb-5 text-center">Nơi lưu giữ những khoảnh khắc đẹp của cộng đồng yêu thiên nhiên Đà
            Nẵng</p>
        <div class="row-change">
            <%if(dataOther3 && dataOther3.products.length > 0){
                dataOther3.products.forEach( prod => {
                    if(prod.showTop === true){
                    name = prod.name;
                    pathImage = prod.images[0]?.image;
            %>
            <div class="cot-12 cot-6-md cot-4-lg">
                <div class="home-hinhanh-img">
                    <a href="<%= name.replace(/ /g , '-')%>-<%= prod.pageId %>.<%= prod._id %>.html">
                        <img src="<%= pathImage%>" alt="<%= name%>">
                    </a>
                    <div class="home-hinhanh-title position-absolute w-100">
                        <a href="<%= name.replace(/ /g , '-')%>-<%= prod.pageId %>.<%= prod._id %>.html">
                            <h4 class="h4-title"><%= name%></h4>
                        </a>
                    </div>
                </div>
            </div>
           <%}})
            }%>
        </div>
        <a class="main-btn mx-auto mt-4" href="hinh-anh">
           <%= '<Xem thêm>'%>
       </a>
    </div>
</div>